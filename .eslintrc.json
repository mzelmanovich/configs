{
    "extends": "fullstack",
    "rules": {
        "semi": [1, "always"],
        "eol-last": "off",
        "class-methods-use-this": [0],
        "no-unused-expressions": ["warn"]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}